           <!-- <article class="row border-top border-bottom"> -->
          <article class="row">
            <br />
              <center>
                <div class="input-append">
                  <input class="span6" id="longlink" size="16" type="text" placeholder="Paste link to shorten" autofocus="autofocus"><button class="btn" type="button" id="submit">Shorten</button>
                </div>


                <div id="spinner" style="display:none;"><img src="assets/images/ajax_spinner.gif"></div><br />
                <h1 class="remove-bottom pad-top-extra-large align-center"><div id="result"></div></h1>


                <script>

                $(document).ready(function(){ 

                   $("#submit").click(function() {

                    $("#spinner").show();
                    $("#result").empty();
                    $("#result").fadeOut("slow", function(){

                    $.get('/shorten', { q: $("#longlink").val() }, function(data){
                      $("#spinner").hide();
                      $("#result").html(data);
                    });
                  });
                  $("#result").fadeIn("slow");
                   
                   });

                });

                </script>




              </center>
